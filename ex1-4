vet1 = [5, 7, 2, 9, 4, 1, 3]
print("a) Tamanho da lista: ", len(vet1))
print("b) Maior valor da lista: ", max(vet1))
print("c) Menor valor da lista: ",min(vet1))
print("d) Soma dos valores da lista: ",sum(vet1))
L.sort()
print("e) Lista em ordem crescente: ", vet1)
L.reverse()
print("f) Lista em ordem decrescente: ", vet1)
